{
	"name": "vscode-plugin-xo-qa-maintenance",
	"displayName": "vscode-plugin-xo-qa-maintenance",
	"description": "HelloWorld example for VS Code",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.38.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.helloWorld"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.helloWorld",
				"title": "Hello World"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.38.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"typescript": "^3.3.1",
		"tslint": "^5.12.1",
		"vscode-test": "^1.2.0"
	},
	"dependencies": {
		"@types/sqlite3": "^3.1.5",
		"@types/tmp": "^0.1.0",
		"@types/unzipper": "^0.10.0",
		"jenkins-api-ts-typings": "^0.8.7",
		"sqlite3": "^4.1.0",
		"tmp": "^0.1.0",
		"typed-rest-client": "^1.5.0",
		"typeorm": "^0.2.19",
		"unzipper": "^0.10.5",
		"winston-decorator": "0.0.8"
	}
}
